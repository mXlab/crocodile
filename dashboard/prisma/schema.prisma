generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Experiment {
  id            Int                   @id @default(autoincrement())
  experiment_id String
  name          String
  created_at    DateTime          
  path          String
  status        String
  updated_at    DateTime?
  models        Model[]
}

model Model {
  id            Int                      @id @default(autoincrement())
  name          String
  type          String
  created_at    DateTime
  uploaded_at   DateTime?
  path          String
  experiment_id Int
  iteration     Int
  fid           Float?
  experiments   Experiment             @relation(fields: [experiment_id], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@index([experiment_id], map: "modeltable_experiment_id")
}
